{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "browsingData",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "desktopCapture",
    "downloads",
    "fontSettings",
    "history",
    "identity",
    "idle",
    "management",
    "nativeMessaging",
    "notificationProvider",
    "notifications",
    "pageCapture",
    "privacy",
    "processes",
    "proxy",
    "sessions",
    "system.display",
    "tabCapture",
    "tabs",
    "topSites",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "https://ajax.googleapis.com/"
  ]
}
